    "scripts": {
      "serve": "serve -s build",
      "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
      "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest",
        "plugin:jsx-a11y/recommended"
      ],
      "plugins": [
        "jsx-a11y"
      ]
    },
    "jest": {
      "collectCoverageFrom": [
        "src/**/*.{js,jsx,ts,tsx}",
        "!<rootDir>/node_modules/"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 50,
          "functions": 50,
          "lines": 50,
          "statements": 50
        }
      },
      "coverageReporters": [
        "text"
      ]
    }
